{
  "port": 8080,
  "serveFiles": true,
  "ssl": {
    "cert": "test.crt",
    "private": "test.key"
  },
  "endpoints": [
    {
      "paths": ["/path1", "/path2"],
      "methods": ["GET"],
      "status": 200,
      "headers": [{
        "Content-Type": "application/json"
      }],
      "body": "{\"id\": 40}",
      "latency": {
          "min": 100,
          "max": 1000
       }
    },
    {
      "paths": ["/path3"],
      "methods": ["GET", "POST"],
      "status": 200,
      "headers": [{
        "Content-Type": "application/json"
      }],
      "body": "{\"name\": \"john\"}"
    },
  ],
  "proxies": [
    {
      "paths": ["/users/{.*}"],
      "destination": "https://api.github.com",
    }
  ]
}
